<!DOCTYPE html>
<html>
<head>
    <title>Fortunes</title>
</head>
<body>
<div>
    <h1>Foo</h1>
    <h2>{{ head }}</h2>
</div>
<table>
    <tbody>
    <tr>
        <th>id</th>
        <th>message</th>
    </tr>
    {{# each fortunes }}
        <tr>
            <td class="col-index">{{index}}</td>
            <td class="col-id">{{id}}</td>
            <td class="col-msg">{{super::head}} {{message}} {{index0}}</td>
            <td class="another"><a class="button">Delete</a></td>
            <td class="col-msg">{{super::head}} {{message}}</td>
            <td class="another"><a class="button">Fol</a></td>
        </tr>
    {{/each }}
    </tbody>
</table>
</body>
</html>

